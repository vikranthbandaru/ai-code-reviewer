{
    "name": "@ai-reviewer/webhook-server",
    "version": "1.0.0",
    "description": "GitHub App webhook receiver for AI Code Reviewer",
    "type": "module",
    "main": "./dist/index.js",
    "scripts": {
        "build": "tsc",
        "dev": "tsx watch src/index.ts",
