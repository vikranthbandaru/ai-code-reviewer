        "jsonwebtoken": "^9.0.0",
        "openai": "^6.17.0",
        "pino": "^9.0.0",
        "pino-http": "^10.0.0",
        "pino-pretty": "^13.1.3",
        "uuid": "^9.0.0",
        "zod": "^3.23.0"
    },
    "devDependencies": {
        "@types/express": "^4.17.0",
        "@types/jsonwebtoken": "^9.0.0",
        "@types/node": "^20.11.0",
